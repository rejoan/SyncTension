{
  "manifest_version": 3,
  "name": "Juwa API",
  "version": "2",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://*/*"],
  "action": {
    "default_popup": "index.html",
	"default_icon": "juwa_extensions.png"
  },
  "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["jquery-3.7.1.min.js","content-script.js"]
   }
 ]
}
